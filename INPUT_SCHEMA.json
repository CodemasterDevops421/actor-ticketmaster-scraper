      "location": {
        "title": "Location",
        "description": "Enter keys in form of ISO Alpha-2 Country Codes. For values, add geoHash or leave blank if you don't want to specify the exact geographical point. You can look up the geoHash of your city or use various tools for conversion of longitude and latitude into geoHash (such as the one at this link: http://geohash.co). Use an arbitrary geoHash precision.",
        "type": "array",
        "editor": "keyValue",
        "placeholderKey": "US",
        "placeholderValue": "dr5regw3pg6ft",
        "uniqueItems": true,
        "sectionCaption": "Events Location",
        "sectionDescription": "Specify countries, exact geographical points or combination of both."
      },
      "distance": {
        "title": "Distance",
        "description": "Distance radius from the given event location",
        "type": "integer",
        "unit": "miles",
        "default": 100,
        "minimum": 0,
        "maximum": 5000
      },
      "allDates": {
        "title": "All Dates",
        "description": "Shows all events.",
        "type": "boolean",
        "default": true,
        "sectionCaption": "Date filter",
        "sectionDescription": "Specify date filter for scraped events. If you want to specify a date range, uncheck 'All Dates' option first and set your chosen option instead."
      },
      "thisWeekendDate": {
        "title": "This Weekend",
        "description": "Filter only those events that take place during this weekend.",
        "type": "boolean",
        "default": false
      },
      "dateRange": {
        "title": "Date Range",
        "description": "Filter only those events that take place during the given date range.",
        "type": "object",
        "editor": "json",
        "prefill": {
          "from": "2021-10-21",
          "to": "2021-10-29"
        },
        "minProperties": 2,
        "maxProperties": 2
      },
      "sort": {
        "title": "Sort by",
        "description": "Choose sort order for scraped events.",
        "type": "string",
        "editor": "select",
        "enum": ["date", "relevance", "distance", "nameAsc", "nameDesc"],
        "enumTitles": ["Date", "Most Popular", "Distance", "Names A-Z", "Names Z-A"],
        "default": "date",
        "sectionCaption": "Sort options",
        "sectionDescription": "Specify sort order in which you want your events to be saved in the dataset."
      },
      "pages": {
        "title": "Number of Pages",
        "description": "Choose number of event search pages you want to scrape. There's normally 10 events on one page.",
        "type": "integer",
        "default": 1,
        "minimum": 1,
        "maximum": 5000,
        "unit": "pages"
      }
