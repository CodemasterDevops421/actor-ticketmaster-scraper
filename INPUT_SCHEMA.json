      "sectionCaption": "Date Filter",
      "sectionDescription": "Specify date filter for scraped events. If you want to specify a date range, uncheck 'All Dates' option first and set your chosen option instead."
    },
    "thisWeekendDate": {
      "title": "This Weekend",
      "description": "Filter only those events that take place during this weekend.",
      "type": "boolean",
      "default": false
    },
    "dateFrom": {
      "title": "From",
      "description": "Filter only those events that take place from this date. Valid format is: YYYY-MM-DD.",
      "type": "string",
      "editor": "textfield",
      "minLength": 8,
      "maxLength": 10
    },
    "dateTo": {
      "title": "To",
      "description": "Filter only those events that take place before this date. Valid format is: YYYY-MM-DD.",
      "type": "string",
      "editor": "textfield",
      "minLength": 8,
      "maxLength": 10
    },
    "includeTBA": {
